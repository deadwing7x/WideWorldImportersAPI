@model IEnumerable<WebApplication1.Models.Sales>

@{
    ViewBag.Title = "GetProfitDataByStateProvince";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>State Province</th>
            <th>Total Sale</th>
            <th>Profit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var sales in Model)
        {
            <tr>
                <td>@sales.stateProvince</td>
                <td>@sales.totalSale</td>
                <td>@sales.profit</td>
            </tr>
        }
    </tbody>
</table>
